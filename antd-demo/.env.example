# Frontend Configuration
REACT_APP_API_URL=http://localhost:12345
REACT_APP_VERSION=1.0.0
PORT=3000

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_API_TIMEOUT=10000

# Authentication Settings
REACT_APP_TOKEN_KEY=arc_workpiece_token
REACT_APP_TOKEN_EXPIRE_MINUTES=30
REACT_APP_REFRESH_INTERVAL=1500000

# Feature Flags
REACT_APP_ENABLE_POINT_CLOUD_PREVIEW=true
REACT_APP_ENABLE_MEASUREMENTS=true
REACT_APP_ENABLE_HISTORY=true

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_PRIMARY_COLOR=#1a365d
REACT_APP_SECONDARY_COLOR=#004599

# Error Reporting
REACT_APP_SENTRY_DSN=
REACT_APP_ERROR_REPORTING=true

# Performance Monitoring
REACT_APP_PERFORMANCE_MONITORING=true
REACT_APP_ANALYTICS_ID=

# Security
REACT_APP_STRICT_MODE=true
REACT_APP_CSP_NONCE=

# Documentation
REACT_APP_DOCS_URL=http://localhost:12345/docs

# Comments for developers:
# - All environment variables must be prefixed with REACT_APP_
# - Values will be embedded during build time
# - Restart dev server after changes
# - Use process.env.REACT_APP_* to access values
